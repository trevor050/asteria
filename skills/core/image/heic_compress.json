{
  "id": "heic_compress",
  "name": "HEIC Compress",
  "version": "1.0.0",
  "author": "asteria",
  "aliases": ["compress heic", "reduce heic size"],
  "category": "compress",
  "description": "Compress HEIC via JPEG quality reduction",
  "inputTypes": [".heic", ".heif"],
  "outputType": ".heic",
  "params": [
    {
      "name": "quality",
      "type": "int",
      "label": "Quality",
      "default": 85,
      "presets": [90, 80, 70, 60],
      "min": 40,
      "max": 100,
      "unit": "%"
    }
  ],
  "driver": "pipeline",
  "isMeta": false,
  "executor": {
    "type": "pipeline",
    "steps": [
      {"skillId": "convert_heic_to_jpeg"},
      {"skillId": "compress"},
      {"skillId": "convert_jpeg_to_heic"}
    ]
  },
  "permissions": ["files.read", "files.write", "files.temp", "tools.exec"],
  "dangerLevel": 0
}
